@page
@model CW18.Areas.AdminArea.Pages.RegisterAdminrModel
<h1>ثبت نام ادمین</h1>
<form method="post">
    <div class="form-row">
        <div class="form-group p-3">
            <label asp-for="@Model.AdminViewModel.FisrtName"></label>
            <input class="form-control pt-1" asp-for="@Model.AdminViewModel.FisrtName">
        </div>
        <div class="form-group p-3">
            <label asp-for="@Model.AdminViewModel.LastName"></label>
            <input class="form-control pt-1" asp-for="@Model.AdminViewModel.LastName">
        </div>
        <div class="form-group p-3">
            <label asp-for="@Model.AdminViewModel.Email"></label>
            <input type="email" class="form-control pt-1" asp-for="@Model.AdminViewModel.Email">
        </div>
        <div class="form-group p-3">
            <label asp-for="@Model.AdminViewModel.Password"></label>
            <input type="password" class="form-control pt-1" asp-for="@Model.AdminViewModel.Password">
        </div>

    </div>
    <button type="submit" class="btn btn-primary">ثبت نام</button>
</form>
